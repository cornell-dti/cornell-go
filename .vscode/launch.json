  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Docker: Attach to Node",
      "localRoot": "${workspaceFolder}/server",
      "remoteRoot": "/app/server",
      "port": 9229
    },
    {
      "name": "game",
      "cwd": "game",
      "request": "launch",
      "type": "dart"
    },
    {
      "name": "game (profile mode)",
      "cwd": "game",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "game (release mode)",
      "cwd": "game",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    }
  ]
}
